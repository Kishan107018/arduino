int a = 1;
int led = 14;
int buzzer=15;
void setup()
{
  pinMode(A0, INPUT);
  pinMode(led, OUTPUT);
  pinMode(buzzer,OUTPUT);
  Serial.begin(9600);
}

void loop()
{
  a = analogRead(A0)-85;
  Serial.println(a);
  if(a>0)
  {
    digitalWrite(led, HIGH);
    tone(buzzer, 1000,10);
  }
  else
  {
    digitalWrite(buzzer, LOW);
    digitalWrite(led,LOW);
  }
  
  delay(1000);
}
